{
  "name": "AbstractItem",
  "plural": "items",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "itemtype": {
      "type": "string",
      "required": true
    },
    "level": {
      "type": "number",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "manufacturerId": {
      "type": "string",
      "required": true
    },
    "rarityId": {
      "type": "string",
      "required": true
    },
    "gibbedCode": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "uniqueText": {
      "type": "string"
    },
    "additionalText": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "manufacturer": {
      "type": "belongsTo",
      "model": "Manufacturer",
      "foreignKey": "manufacturerId"
    },
    "rarity": {
      "type": "belongsTo",
      "model": "Rarity",
      "foreignKey": "rarityId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
